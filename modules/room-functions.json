{
  "createRoom": {
    "handler": "room/createRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/createRoom",
          "method": "post"
        }
      }
    ]
  },
  "deleteRoom": {
    "handler": "room/deleteRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/deleteRoom/{id}",
          "method": "delete"
        }
      }
    ]
  },
  "listRoom": {
    "handler": "room/listRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/listRoom",
          "method": "get"
        }
      }
    ]
  }
}