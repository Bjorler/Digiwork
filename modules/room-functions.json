{
  "createRoom": {
    "handler": "room/createRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/create",
          "method": "post",
          "cors": true
        }
      }
    ]
  },
  "getRoom": {
    "handler": "room/getRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/get/{id}",
          "method": "get",
          "cors": true
        }
      }
    ]
  },
  "deleteRoom": {
    "handler": "room/deleteRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/delete/{id}",
          "method": "delete",
          "cors": true
        }
      }
    ]
  },
  "listRoom": {
    "handler": "room/listRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/list",
          "method": "get",
          "cors": true
        }
      }
    ]
  },
  "updateRoom": {
    "handler": "room/updateRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/update/{id}",
          "method": "put",
          "cors": true
        }
      }
    ]
  },
  "exportRoom": {
    "handler": "room/exportRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/export",
          "method": "get",
          "cors": true
        }
      }
    ],
    "layers": [
      "arn:aws:lambda:us-east-1:565183114770:layer:common-core:25",
      "arn:aws:lambda:us-east-1:565183114770:layer:common-csv-writer:1"
    ]
  },
  "reservationsRoom": {
    "handler": "room/reservationsRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/check-reservations/{id}",
          "method": "get",
          "cors": true
        }
      }
    ]
  },
  "catalogueRoom": {
    "handler": "room/catalogueRoom.handler",
    "events": [
      {
        "http": {
          "path": "room/catalogue",
          "method": "get"
        }
      }
    ]
  }
}