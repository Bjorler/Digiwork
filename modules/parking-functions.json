{
  "create": {
    "handler": "parking/createParking.handler",
    "events": [
      {
        "http": {
          "path": "parking/create",
          "method": "post",
          "cors": true
        }
      }
    ]
  },
  "deleteparking": {
    "handler": "parking/deleteParking.handler",
    "events": [
      {
        "http": {
          "path": "parking/delete/{id}",
          "method": "delete",
          "cors": true
        }
      }
    ]
  },
  "getparking": {
    "handler": "parking/getParking.handler",
    "events": [
      {
        "http": {
          "path": "parking/get/{id}",
          "method": "get",
          "cors": true
        }
      }
    ]
  },
  "listparking": {
    "handler": "parking/listParking.handler",
    "events": [
      {
        "http": {
          "path": "parking/list",
          "method": "get",
          "cors": true
        }
      }
    ]
  },
  "updateparking": {
    "handler": "parking/updateParking.handler",
    "events": [
      {
        "http": {
          "path": "parking/update/{id}",
          "method": "put",
          "cors": true
        }
      }
    ]
