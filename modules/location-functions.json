{
  "createLocation": {
    "handler": "location/createLocation.handler",
    "events": [
      {
        "http": {
          "path": "location/createLocation",
          "method": "post"
        }
      }
    ]
  },
  "getLocation": {
    "handler": "location/getLocation.handler",
    "events": [
      {
        "http": {
          "path": "location/getLocation/{id}",
          "method": "get"
        }
      }
    ]
  },
  "updateLocation": {
    "handler": "location/updateLocation.handler",
    "events": [
      {
        "http": {
          "path": "location/updateLocation/{id}",
          "method": "put"
        }
      }
    ]
  },
  "listLocation": {
    "handler": "location/listLocation.handler",
    "events": [
      {
        "http": {
          "path": "location/listLocation",
          "method": "get"
        }
      }
    ]
  },
  "deleteLocation": {
    "handler": "location/deleteLocation.handler",
    "events": [
      {
        "http": {
          "path": "location/deleteLocation/{id}",
          "method": "delete"
        }
      }
    ]
  }
}